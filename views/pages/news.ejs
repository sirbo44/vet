<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head'); %>
    <style>  
        #col{
            margin-top: 1.18%;
            margin-bottom: 2.37%;
        }
        #row-cols-3{
            margin-left: 8.3%;
        }
    </style>
<body>
    <%- include('../partials/navbar'); %>
    <div class="row row-cols-3" id="row-cols-3">
        <% for(var i = 0; i<data.length; i++ ) {%>
        <div class="col">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <p class="card-title"><b><%= data[i]['title'] %></b></p>
                    <p class="card-text"><%= data[i]['content'] %></p>
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <%- include('../partials/footer'); %>
    <%- include('../partials/bottom'); %>
</body>
</html>